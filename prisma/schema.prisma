// docs: https://pris.ly/d/prisma-schema

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuarios {
  id       String  @id @default(uuid())
  nome     String
  email    String  @unique
  username String  @unique
  password String
  salt     String?
}
